}
 
#include <stdlib.h>
#include <stdio.h>
#include <math.h>
int main()
{
    int a = 1;
    for (;;)
    {
        printf("%d\n", log(a));
        if (a == 0)
            break;
        ++a;
    }
    return 0;
}
 
