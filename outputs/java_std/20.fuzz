import java.lang.Object;
import java.util.Scanner;
public class HelloIndian {
	public static void main1(String[] args) {
		Scanner s1 = new Scanner("Hello Indian\nworld");
		do {
			if(!s1.hasNext())
				continue; 
			System.out.println(s1.nextLine());
					
			if (true)
			  break;
			else System.exit(0);
			
		} while(s1.hasNextLine());
	}
}
