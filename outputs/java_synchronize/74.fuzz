import java.lang.Object;
class Sample {
    void doSomething2() {
        synchronized(this) {
            System.out.println(this + ": doSomething2()");
        }
    }
}

class SynchTest2 {
    public static void main(String [] args) {
        Sample s = new Sample();
        s.doSomething2();
    }
}

 
