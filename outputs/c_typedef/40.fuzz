#include <stdlib.h>
#include <stdio.h>
struct Date
{
	int day;
	int month;
	int year;
};
int main()
{
	struct Date date;
	printf("Enter Date : ");
	scanf("%d/%d/%d", &date.day, &date.month, &date.year);
	printf("date.day:%d,date.month:%d,date.year:%d\n", date.day, date.month, date.year);
	return 0;
}
